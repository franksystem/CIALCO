package ec.gob.magap.exception;

public class MagapException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 4746889883370101408L;

	public MagapException() {
		super();
	}

	public MagapException(String message) {
		super(message);

	}

	public MagapException(Throwable ex) {
		super(ex);

	}

	public MagapException(String mensaje, Throwable causa) {
		super(mensaje, causa);

	}
}